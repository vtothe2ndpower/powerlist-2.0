eturn { ...item, completed: !item.completed }\r\n      }\r\n      return item;\r\n    });\r\n    this.setState({\r\n      todos: updatedTodos\r\n    });\r\n  }\r\n\r\n  render() {\r\n    const { todos } = this.state;\r\n    return (\r\n      <div className=\"TodoList\">\r\n        <h1>The Power List <span>A Simple React Todo List Application</span></h1>\r\n        {todos.map((item, index) => (\r\n          <Todo \r\n            key={item.id}\r\n            id={item.id}\r\n            task={item.task} \r\n            completed={item.completed}\r\n            num={index}\r\n            updateItem={this.update}\r\n            toggleTodo={this.toggleCompletion}\r\n            handleClick={() => this.handleClick(item.id)}\r\n          />\r\n        ))}\r\n        <NewTodoForm addItem={this.addItem} />\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nexport default TodoList;\r\n\r\n// Video 4:\r\n// Add Reactstrap Code\r\n// Add CSS for transitions","C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\components\\NewTodoForm.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\components\\Todo.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\components\\AppNavbar.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\store.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\reducers\\index.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\reducers\\todoReducer.js",["58"],"import { v4 as uuid } from 'uuid';\r\nimport { GET_TODOS, ADD_TODO, DELETE_TODO } from '../actions/types';\r\n\r\nconst initialState = {\r\n  todos: [\r\n    { task: 'Aesthe