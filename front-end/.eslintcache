r\n    });\r\n    this.setState({\r\n      todos: updatedTodos\r\n    });\r\n  }\r\n\r\n  toggleCompletion(id) {\r\n    const updatedTodos = this.state.todos.map(item => {\r\n      if(item.id === id) {\r\n        return { ...item, completed: !item.completed }\r\n      }\r\n      return item;\r\n    });\r\n    this.setState({\r\n      todos: updatedTodos\r\n    });\r\n  }\r\n\r\n  render() {\r\n    // const { todos } = this.state;\r\n    const { todos } = this.props.todo;\r\n    return (\r\n      <div className=\"TodoList\">\r\n        <h1>The Power List <span>A Simple React Todo List Application</span></h1>\r\n        {todos.map((item, index) => (\r\n          <Todo \r\n            key={item._id}\r\n            id={item._id}\r\n            task={item.task} \r\n            completed={item.completed}\r\n            num={index}\r\n            updateItem={this.update}\r\n            toggleTodo={this.toggleCompletion}\r\n            handleClick={() => this.handleClick(item._id)}\r\n          />\r\n        ))}\r\n      </div>\r\n    )\r\n  }\r\n}\r\n\r\nTodoList.propTypes = {\r\n  getTodos: PropTypes.func.isRequired,\r\n  todo: PropTypes.object.isRequired\r\n}\r\n\r\nconst mapStateToProps = (state) => ({\r\n  todo: state.todo\r\n});\r\n\r\nexport default connect(mapStateToProps, { getTodos, deleteTodo })(TodoList);\r\n\r\n// Video 4:\r\n// Add Reactstrap Code\r\n// Add CSS for transitions","C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\components\\Todo.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\components\\AppNavbar.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\store.js",[],"C:\\Software Developer\\Programming Classes\\Online Courses\\The Modern React Bootcamp\\powerlist-web\\front-end\\src\\reducers\\index.js",[],"C:\\Software Developer\